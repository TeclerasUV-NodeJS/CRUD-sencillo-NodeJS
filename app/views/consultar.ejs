<% include header %>

<!-- for datos -->
<%for(i in datos){%>
  nombre:
  <br>
  <%=datos[i].nombre%>
  <br>
  Apellido:
  <%=datos[i].apellido%>
  <br>
<%}%>
<br>
<br>
Asignaturas:
<form action="/administrar_asignatura" class="" method="get">

  <%for(i in asignaturas){%>
    <hr>
    <a href=<%=asignaturas[i].enlace%>>Ver datos de la asignatura</a>
    <input class="form-control" type="radio" name="idasignatura" value=<%=asignaturas[i].id%>>
    nombre:
    <br>
    <%=asignaturas[i].id%>
    <br>
    Nombre:
    <%=asignaturas[i].nombre%>
    Codigo:
    <%=asignaturas[i].codigo%>
    <br>
    <hr>
  <%}%>
  <input class="form-control" name="name" type="submit" value="Ir a la asignatura seleccionada">
</form>
<% include footer %>
